<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FAQs"
                    android:textColor="#000000"
                    android:id="@+id/textView1"
                    android:textSize="40dp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="20dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1.How do I contact anyone in the university ?"
                    android:id="@+id/textView2"
                    android:textSize="15dp"
                    android:textColor="#1326f8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView3"
                    android:textSize="15dp"
                    android:textColor="#060606"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2.How do I get to know about Faculties teaching in particular courses ?"
                    android:id="@+id/textView4"
                    android:textSize="15dp"
                    android:textColor="#1326f8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView5"
                    android:textSize="15dp"
                    android:textColor="#060606"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="3.What is the use of ‘Notifications’ ?"
                    android:id="@+id/textView6"
                    android:textSize="15dp"
                    android:textColor="#1326f8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView7"
                    android:textSize="15dp"
                    android:textColor="#060606"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="4.From where I can get to know about the Constituent Colleges under Rajasthan University ?"
                    android:id="@+id/textView8"
                    android:textSize="15dp"
                    android:textColor="#1326f8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView9"
                    android:textSize="15dp"
                    android:textColor="#060606"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="5.How do I know about the Sessional Information of the college ?"
                    android:id="@+id/textView10"
                    android:textSize="15dp"
                    android:textColor="#1326f8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView11"
                    android:textSize="15dp"
                    android:textColor="#060606"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="6.What is the use of this application ?"
                    android:id="@+id/textView12"
                    android:textSize="15dp"
                    android:textColor="#1326f8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView13"
                    android:textSize="15dp"
                    android:textColor="#060606"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="7.How do I get to know about Latest news and events of the college ?"
                    android:id="@+id/textView14"
                    android:textSize="15dp"
                    android:textColor="#1326f8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView15"
                    android:textSize="15dp"
                    android:textColor="#060606"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="8.Is there any other method to get all these information ?"
                    android:id="@+id/textView16"
                    android:textSize="15dp"
                    android:textColor="#1326f8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView17"
                    android:textSize="15dp"
                    android:textColor="#060606"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9.Can I also share this application to others ?"
                    android:id="@+id/textView18"
                    android:textSize="15dp"
                    android:textColor="#1326f8" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView19"
                    android:textSize="15dp"
                    android:textColor="#060606"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>